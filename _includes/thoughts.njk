{% include '_head.html' %} {% include '_nav.html' %}
<div class="main">
  <div class="postsList">
    {%- for post in collections.Thoughts | reverse -%}
    <div class="postsList__item">
      <header class="postsList__itemHeader">
        <h2 class="postsList__title">
          <a class="postsList__link" href="{{ post.url }}" rel="bookmark">
            {{ post.data.title }}
          </a>
        </h2>

        <div class="postsList__date">
          {{ post.date.toUTCString() }}
        </div>
      </header>
    </div>
    {%- endfor -%}
  </div>
</div>
{% include '_footer.html' %}
