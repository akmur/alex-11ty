{% include '_head.html' %} {% include '_nav.html' %}
<div id="main" class="main">
  <article class="post">
    <header class="postHeader">
      {% if title %}
      <h1 class="postContent__title">
        {{ title }}
      </h1>
      {% endif %} {% if date %}
      <div class="postContent__date">Posted on {{ date.toUTCString() }}</div>
      {% endif %}
      <!-- .entry-meta -->
    </header>
    {% if featured_image %}
    <div class="postContent__image">
      <div class="postContent__thumbnail">
        <img src="{{ featured_image }}" />
      </div>
    </div>
    {% endif %}
    {{ content | safe }}
  </article>
</div>
{% include '_footer.html' %}
