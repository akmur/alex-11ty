{
  "name": "eleventy-base-blog",
  "version": "5.0.2",
  "description": "The repository for my eleventy blog, based on eleventy-base-blog by Zach Leatherman.",
  "scripts": {
    "build": "eleventy",
    "watch": "eleventy --watch",
    "dev": "eleventy --serve --port=8888",
    "styles": "node-sass --watch --output-style nested ./scss/index.scss ./css/index.css",
    "debug": "DEBUG=* eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Alessandro Muraro",
    "email": "akmuraro@gmail.com",
    "url": "https://alexmuraro.me/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "luxon": "^1.25.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "node-sass": "^5.0.0"
  }
}
